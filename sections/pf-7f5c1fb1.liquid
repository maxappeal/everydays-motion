                    'pregnant': (document.getElementById('pregnant').checked) ? 'yes' : 'no',
                    'vegan': (document.getElementById('vegan').checked) ? 'yes' : 'no',
                    'pills': roundTotal,
                    'gram': totalPerDay.toFixed(2) + 'g',
                    'price': price + '€ / Tag'
                }
            });

        }

